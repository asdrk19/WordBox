<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dictionary_arka1_1"
    tools:context=".WordBox_dictionary">


    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-109dp"
        android:layout_marginBottom="-3dp"
        android:text="Kaydettiğiniz Cümlelerin Olduğu Sayfayı Açar"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView19"
        app:layout_constraintStart_toStartOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="105dp"
        android:layout_height="100dp"
        android:layout_marginStart="-77dp"
        android:layout_marginBottom="-13dp"
        android:rotation="166"
        android:rotationY="-182"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintStart_toStartOf="@+id/button11"
        app:srcCompat="@drawable/ok" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="111dp"
        android:layout_height="59dp"
        android:layout_marginStart="-33dp"
        android:layout_marginBottom="-19dp"
        android:gravity="center"
        android:text="Veriyi Sözlükten Siler"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="@+id/imageView12" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="-85dp"
        android:layout_marginTop="-3dp"
        android:text="Kaydettiğiniz Veriyi Sesli Okur"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="115dp"
        android:layout_height="41dp"
        android:layout_marginStart="55dp"
        android:layout_marginBottom="-11dp"
        android:text="Veri Eklemenizi Sağlar"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/imageView13"
        app:layout_constraintEnd_toEndOf="@+id/imageView13"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView13" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="89dp"
        android:layout_height="77dp"
        android:layout_marginStart="-75dp"
        android:layout_marginBottom="-35dp"
        android:rotation="182"
        android:rotationY="-194"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintStart_toStartOf="@+id/cardView"
        app:srcCompat="@drawable/ok" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/cardview_selector"
        android:foreground="?attr/selectableItemBackground"
        app:cardBackgroundColor="#86A59C"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="ADD"
                android:textColor="@color/black"
                android:textStyle="bold" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="35dp"
        android:background="@drawable/rounded_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.0411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent="0.1019"
        app:srcCompat="@android:drawable/ic_menu_revert" />


    <ScrollView
        android:id="@+id/scrollView5"
        android:layout_width="378dp"
        android:layout_height="256dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="290dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.355"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.941">

        <LinearLayout
            android:id="@+id/Ana_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/dictionary_onboard_ss"
                tools:layout_editor_absoluteX="27dp"
                tools:layout_editor_absoluteY="16dp" />

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="9dp"
        android:clickable="false"
        android:text="Sentences>"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="96dp"
        android:layout_height="107dp"
        android:layout_marginBottom="-65dp"
        android:rotation="133"
        app:layout_constraintBottom_toBottomOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="@+id/scrollView5"
        app:srcCompat="@drawable/ok" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="95dp"
        android:layout_height="92dp"
        android:layout_marginStart="245dp"
        android:layout_marginTop="-69dp"
        android:rotation="-24"
        app:layout_constraintStart_toStartOf="@+id/scrollView5"
        app:layout_constraintTop_toBottomOf="@+id/scrollView5"
        app:srcCompat="@drawable/ok" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="35dp"
        android:layout_weight="2"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.0551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent="0.2991">
        >

        <androidx.cardview.widget.CardView
            android:id="@+id/kart_kay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/cardview_selector"
            android:clickable="true"
            android:foreground="?attr/selectableItemBackground"
            android:onClick="next_page"
            app:cardCornerRadius="15dp"
            app:layout_constraintBottom_toTopOf="@+id/imageButton14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.421"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintWidth_percent="0.201">

            <TextView
                android:id="@+id/kay"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Haydi Başlayalım!"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:textStyle="bold" />

        </androidx.cardview.widget.CardView>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>